{
  "name": "Ladybugüêû APIs",
  "version": "v2.0.0",
  "description": "A powerful and flexible API server with YouTube, Music, and Media processing capabilities",
  "author": "Ntando Mods",
  "website": "https://your-api.onrender.com",
  "documentation": "https://your-api.onrender.com/docs",
  "apiSettings": {
    "creator": "Ntando Mods",
    "contact": "support@ladybugapi.com",
    "rateLimit": {
      "enabled": true,
      "max": 100,
      "windowMs": 900000,
      "message": "Too many requests, please try again later"
    },
    "cors": {
      "enabled": true,
      "origin": "*"
    },
    "cache": {
      "enabled": true,
      "ttl": 3600
    }
  },
  "features": {
    "youtubeDownload": true,
    "musicSearch": true,
    "videoProcessing": true,
    "mediaConversion": true,
    "analytics": true
  },
  "categories": [
    {
      "name": "General",
      "description": "General purpose APIs",
      "icon": "üîß",
      "routes": [
        {
          "path": "/example",
          "name": "Example API",
          "description": "Example API endpoint",
          "method": ["GET", "POST"]
        }
      ]
    },
    {
      "name": "Utility",
      "description": "Utility and tools APIs",
      "icon": "üõ†Ô∏è",
      "routes": [
        {
          "path": "/weather",
          "name": "Weather API",
          "description": "Get weather information for any city",
          "method": ["GET", "POST"],
          "params": ["city", "unit"]
        },
        {
          "path": "/qrcode",
          "name": "QR Code Generator",
          "description": "Generate QR codes",
          "method": ["GET", "POST"],
          "params": ["text", "size", "format"]
        },
        {
          "path": "/shorturl",
