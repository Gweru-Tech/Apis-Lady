      "icon": "‚ÑπÔ∏è",
      "routes": [
        {
          "path": "/crypto",
          "name": "Cryptocurrency Prices",
          "description": "Get real-time crypto prices",
          "method": ["GET", "POST"],
          "params": ["symbol", "currency"]
        },
        {
          "path": "/ipinfo",
          "name": "IP Information",
          "description": "Get detailed IP information",
          "method": ["GET", "POST"],
          "params": ["ip"]
        }
      ]
    },
    {
      "name": "YouTube & Media",
      "description": "YouTube download and media APIs",
      "icon": "üì∫",
      "routes": [
        {
          "path": "/ytsearch",
          "name": "YouTube Search",
          "description": "Search YouTube videos",
          "method": ["GET", "POST"],
          "params": ["query", "limit", "type"]
        },
        {
          "path": "/ytmp3",
